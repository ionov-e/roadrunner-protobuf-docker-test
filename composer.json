{
  "name": "ionov-e/roadrunner-protobuf-docker",
  "description": "A testing project to try RoadRunner with protobuf using Docker",
  "keywords": ["roadrunner", "docker", "protobuf"],
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "ionov-e",
      "email": "ioncurly@gmail.com"
    }
  ],
  "require": {
    "php": "^8.3",
    "nyholm/psr7": "^1.8",
    "spiral/roadrunner": "v2024.1.1",
    "spiral/roadrunner-cli": "3.x-dev",
    "spiral/roadrunner-http": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
